@inproceedings{Zitterbart:89,
  AUTHOR =   "Martina Zitterbart",
  TITLE =   "{High-Speed Protocol Implementations Based on a
                  Multiprocessor-Architecture}",
  BOOKTITLE =   "Proceedings of the 1st International Workshop on
                  High-Speed Networks",
  MONTH =   may,
  YEAR =   1989,
  PAGES =   "151--163",
  ABSTRACT =   "The development of high-speed local area networks
                  (HSLANs) imposes new requirements on the
                  implementation of communication
                  protocols. Multi-processor architectures will be
                  well suited to increase the performance of those
                  systems. In this paper a multiprocessor architecture
                  for the implementation of the OSI communication
                  protocols realizing transport systems is
                  presented. The design is based on a horizontal and
                  vertical subdivision of communication systems. The
                  parallel architecture consists of several building
                  blocks, (e.g., pipeline, array of processors) and
                  supports multiple memory concepts (local and global
                  memory). To further increase the performance of a
                  layer, so called layer machines (send and receive
                  part of a layer) can operate in parallel within the
                  same layer and the computing of different layers can
                  be overlapped. First prototype implementations of
                  the OSI Transport Protocol class 4 and the OSI
                  network layer are described, and some simulation
                  results are presented."
}

@article{Zitterbart:91,
  AUTHOR =   "Martina Zitterbart",
  TITLE =   "{High-Speed Transport Components}",
  JOURNAL =   "IEEE Network Magazine",
  YEAR =   1991,
  MONTH =   jan,
  PAGES =   "54--63",
  ABSTRACT =   "This paper gives an overview of the research of
                  high-speed transport components, considering
                  protocols as well as implementations. It also
                  focuses on the use of parallelism to increase the
                  performance of communication nodes. Also, a parallel
                  implementation of OSI protocols on transputer
                  networks based on parallel concepts discussed
                  previously is presented."
}

@inproceedings{Zitterbart:92a,
  AUTHOR =   "Martina Zitterbart",
  TITLE =   "{Parallel Protocol Implementations on Transputers --
                  Experiences with OSI TP4, OSI CLNP, and XTP}",
  BOOKTITLE =   "Proceedings of the First IEEE Workshop on the
                  Architecture and Implementation of High Performance
                  Communication Subsystems",
  MONTH =   feb,
  YEAR =   1992,
  ABSTRACT =   "The demand for high performance communication
                  subsystems has rapidly increased during the last few
                  years. Emerging applications ({\em e.g.},
                  multi-media applications) requiring high perfomrance
                  are one of the driving factors for this
                  development. But still there is a performance gap of
                  at least one order of magnitude between the
                  bandwidth available on the network and the
                  application requirements. This is based on the
                  insufficient processing power inside communication
                  nodes. They cannot serve networks with data rates in
                  the range of Mbps to Gbps. To overcome this
                  so-called transport bottleneck high performance
                  architectures for communication subsystems have to
                  be designed considering hardware and software
                  structure as well as protocol design and service
                  requirements."
}

@techreport{Zitterbart:92b,
  AUTHOR =   "Martina Zitterbart and Burkhard Stiller and Ahmed
                  Tantawy",
  TITLE =   "{A Model for Flexible High-Performance Communication
                  Subsystems}",
  INSTITUTION =   "IBM Research Division",
  YEAR =   1992,
  MONTH =   feb,
  NUMBER =   "TC 17801 (78023)",
  ABSTRACT =   "Many emerging applications require very diverse
                  services from the underlying communication
                  subsystem. Future communication nodes will have to
                  offer some flexibility in selecting service
                  characteristics and efficiency in implementing
                  them. In this paper, we present a model that allows
                  applications to request the communication service
                  that they need. Based on service requirements and on
                  available resources, a suiteable protocol machine is
                  configured to serve the applications. This
                  configuration is done using a proper combination of
                  protocol functions. The conept of layering is
                  therefore abandoned for both flexibility and
                  efficiency reasons."
}

