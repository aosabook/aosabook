@techreport{Liskov:88c,
  AUTHOR =   "Barbara Liskov and Toby Bloom and David Gifford and
                  Robert Scheirfler and William Weihi",
  TITLE =   "{Communication in the Mercury System}",
  INSTITUTION =   "MIT",
