@article{Fraser:83,
  AUTHOR =   "A. G. Fraser",
  TITLE =   "{Towards a Universal Data Transport System}",
  JOURNAL =   ieeejsac,
  MONTH =   nov,
  YEAR =   1983,
  VOLUME =   "SAC-1",
  PAGES =   "803--816",
  ABSTRACT =   "It is anticipated that a ubiquitous data transport
                  system will require integration of local area and
                  wide area networks. The combined network will need
                  to present a uniform appearance to the user, be
                  effective as a transport mechanism for a great
                  variety of traffic patterns, and be economically
                  appropriate for a wide range of consumer
                  products. Learning from telephony and from
                  experiments with a local area network, we conclude
                  that the transport system must seek a clean
                  separation of function and protocol between the
                  network and its users. This separation is achieved
                  by a byte-stream architecture that carries control
                  and data bytes over switched virtual circuits. A
                  DATAKIT packet switch demonstrates how the
                  byte-stream concept can integrate local area and
                  wide area network objectives. This switch is an
                  assemble of interface modules connected by a pair of
                  short passive buses. Each type of interface module
                  serves one type of remote equipment and, if need be,
                  terminates the protocol of that equipment. There are
                  interface modules for trunks that lead to other
                  packet switches, for terminals, and for host
                  computers. Other modules provide system timing,
                  switching, network control, and maintenance
                  support."
}

@article{Fraser:89a,
  AUTHOR =   "A. G. Fraser and W. T. Marshall",
  TITLE =   "{Data Transport in a Byte-Stream Network}",
  JOURNAL =   ieeejsac,
  MONTH =   sep,
  YEAR =   1989,
  VOLUME =   "SAC-7",
  PAGES =   "1020--1033",
  ABSTRACT =   "A telecommunications network that is shared among
                  many users must transport information that
                  originates in many different protocols, embodies
                  many different data representations, and meets a
                  range of performance requirements. The data
                  structure that can simultaneously support all these
                  different needs is a byte stream in which each byte
                  is distinguished as a byte of user information or as
                  a byte of control information. A transport layer
                  protocol optimized for use in a byte stream network
                  is necessarily different from one in which the
                  indestructible unit of information is a packet. A
                  byte stream network has a different error
                  characteristic that implies a different method of
                  error detection, and the fine grain multiplexing
                  that is possible in a byte stream network leads to
                  new opportunities for performance optimization. The
                  Universal Receiver for Datakit networks is a
                  protocol engine that responds to commands which are
                  embedded in the incoming byte stream. By employing
                  transmitters that send different command sequences
                  and respond in various ways to the responses of the
                  Universal Receiver, one can efficiently achieve a
                  range of transport capabilities without loss of
                  connectivity among network users."
}

@inproceedings{Fraser:89b,
  AUTHOR =   "A.G. Fraser",
  TITLE =   "{The Universal Receiver Protocol}",
  BOOKTITLE =   "Proceedings of the 1st International Workshop on
                  High-Speed Networks",
  MONTH =   may,
  YEAR =   1989,
  PAGES =   "19--25",
  ABSTRACT =   "The Universal Receiver Protocol is a lightweight
                  protocol designed to be an efficient means of
                  encapsulating transport protocols. It has also been
                  used successfully in its own right as a protocol for
                  data transport. Low queuing delay demands that
                  statistical multiplexors use relatively small
                  packets, but host computers show best throughput
                  when they work on large blocks of data. The
                  Universal Receiver Protocol is not packet oriented
                  so that it does not depend on the network choice of
                  packet size for efficient multiplexing. Instead, the
                  protocol is designed for a particular style of
                  pipelined receiver implementation that performs
                  block assemble in hardware and delivers completed
                  blocks to the host computer."
}

