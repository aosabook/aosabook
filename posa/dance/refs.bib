@inproceedings{Zukowski:91a,
  TITLE =   "{VLSI implementations of ATM buffer management}",
  AUTHOR =   {Charles Zukowski and Tong-Bi Pei},
  BOOKTITLE =   icc,
  YEAR =   {1991},
  ABSTRACT =   "A semi-shared buffering scheme is introduced. The
                  motivation is to avoid the complexity of maintining
                  linked lists for each queue. This paper shows how a
                  few registers per queue can be used to attain
                  performance close to that of shared
                  buffering. Hardware implementation costs for three
                  buffering schemes (separated, shared, and
                  semi-shared) are discussed. Semi-shared takes
                  significantly more area than shared buffering
                  becaused of additional memory required to achieve
                  the same loss rate. However, the throughput of fully
                  shared buffering is limited by the control circuitry
                  and can be slower than the semi-shared scheme. ",
  TOPIC =   "Buffering semi-shared",
}

@incollection{Zweig:90,
  AUTHOR =   "Jonathan M. Zweig",
  TITLE =   "{The Conduit: a Communication Abstraction in C++}",
  BOOKTITLE =   "Proceedings of the $2^{nd}$ USENIX C++ Conference",
  PUBLISHER =   "USENIX Association",
  MONTH =   apr,
  YEAR =   {1990},
  PAGES =   "191--203",
  ANNOTE =   "Read 12/21/91 (ds)",
  ABSTRACT =   "This paper describes the Conduit, a new abstraction
                  for bidirectional communication that is being used
                  to implement a family of network protocols as part
                  of the Choices operating system project. The use of
                  C++ makes bit and byte-level manipulations easy, at
                  the same time allowing the programmer to exploit
                  object-oriented features such as type-checking,
                  inheritance and polymorphism. The structure imposed
                  on the system by the class hierarchy makes the code
                  easier to understand and permits reuse of both code
                  and interface. The result is a flexible, extensible
                  system that can easily be tuned to provide optimum
                  performance. Certain shortcomings of C++ that are
                  encountered in implementing a communications
                  subsystem are also discussed."
}

